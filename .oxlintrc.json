{
  "plugins": ["typescript", "unicorn", "import", "promise", "vue", "eslint", "vitest"],
  "categories": {
    "correctness": "error",
    "suspicious": "warn"
  },
  "rules": {
    "default-case": "warn",
    "no-empty": "error",
    "no-empty-function": ["error", { "alllow": ["arrowFunctions"] }]
  }
}
